{
  "dependencies": {
    "@tailwindcss/cli": "^4.1.10",
    "alpinejs": "^3.14.9",
    "tailwindcss": "^4.1.10"
  },
  "name": "projectuas",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev:css": "npx @tailwindcss/cli -i src/resources/style/input.css -o src/resources/style/output.css --watch",
    "dev:js": "npx esbuild src/resources/js/app.js --bundle --outfile=src/resources/js/alpine.bundle.js --watch",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:js\""
